        <arguments>
          <argument>
            <kind>1</kind>
            <string>// DVIZHENJE NAKOSO
biboTime++;
if(biboTime &gt; 300)
{
    audio_play_sound(snd_bibo, 1, false);
    biboTime = 0;
}

if(keyboard_check(ord("W")) &amp;&amp; keyboard_check(ord("A")))
{
    image_angle = 45;
    hspeed = -4;
    vspeed = -4;
    
    obj_shadow_fire.image_angle = 45;
    obj_shadow_fire.hspeed = -4;
    obj_shadow_fire.vspeed = -4;
}

if(keyboard_check(ord("W")) &amp;&amp; keyboard_check(ord("D")))
{
    image_angle = 360 - 45;
    hspeed = 4;
    vspeed = -4;
    
    obj_shadow_fire.image_angle = 360 - 45;
    obj_shadow_fire.hspeed = 4;
    obj_shadow_fire.vspeed = -4;
}

if(keyboard_check(ord("S")) &amp;&amp; keyboard_check(ord("A")))
{
    image_angle = 180 - 45;
    hspeed = -4;
    vspeed = 4;
    
    obj_shadow_fire.image_angle = 180 - 45;
    obj_shadow_fire.hspeed = -4;
    obj_shadow_fire.vspeed = 4;
}

if(keyboard_check(ord("S")) &amp;&amp; keyboard_check(ord("D")))
{
    image_angle = 180 + 45;
    hspeed = 4;
    vspeed = 4;
    
    obj_shadow_fire.image_angle = 180 + 45;
    obj_shadow_fire.hspeed = 4;
    obj_shadow_fire.vspeed = 4;
}

// ANIMACIJA NA SPRAJT

if(keyboard_check_pressed(ord("W")) || keyboard_check_pressed(ord("S")) || keyboard_check_pressed(ord("A")) || keyboard_check_pressed(ord("D")))
{
    image_speed = 20;
}
if(keyboard_check_released(ord("W")) || keyboard_check_released(ord("S")) || keyboard_check_released(ord("A")) || keyboard_check_released(ord("D")))
{
    image_speed = 0;
}

// SKOK

if(bJumpUp != 0)
    image_speed = 0;

if(bJumpUp == 1)
{
    image_xscale += 0.002;
    image_yscale += 0.002;
    
    obj_shadow_fire.x += 0.8;
    obj_shadow_fire.y += 0.8;
    
    if(image_xscale == scaleStart + jHeight)
        bJumpUp = 2;
}

if(bJumpUp == 2)
{
    image_xscale -= 0.002;
    image_yscale -= 0.002;
    
    obj_shadow_fire.x -= 0.8;
    obj_shadow_fire.y -= 0.8;
    
    if(image_xscale == scaleStart)
        bJumpUp = 0;
}

// Check collision

// Check for no collision (fall down)
if (place_free(x, y) &amp;&amp; image_xscale &lt;= 0.05)
{
    // Player dead
    alarm[1] = 1;
}

global.itemUnder = collision_point( x, y, ice_platform, false, true );

if (global.itemUnder &amp;&amp; image_xscale == 1)
{
    global.hvar--;
    health = floor(global.hvar/6);
    
    if (health &lt;= 0)
    {
   //     room_restart();
        global.livesLeft --;
      /*  
        if (global.livesLeft == 0)
        {
            room_goto(5);
        }
        else
        {*/
            script_execute(checkpointReset);
     //   }
        
   

    //    health = 100;
    }
}
</string>
