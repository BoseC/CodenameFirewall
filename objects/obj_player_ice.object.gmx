        <arguments>
          <argument>
            <kind>1</kind>
            <string>// DVIZHENJE NAKOSO
if(keyboard_check(vk_up) &amp;&amp; keyboard_check(vk_left)) 
{ 
    image_angle = 45; 
    hspeed = -4; 
    vspeed = -4; 
    
    obj_shadow_ice.image_angle = 45; 
    obj_shadow_ice.hspeed = -4; 
    obj_shadow_ice.vspeed = -4; 
} 

if(keyboard_check(vk_up) &amp;&amp; keyboard_check(vk_right)) 
{ 
    image_angle = 360 - 45; 
    hspeed = 4; 
    vspeed = -4; 
    
    obj_shadow_ice.image_angle = 360 - 45; 
    obj_shadow_ice.hspeed = 4; 
    obj_shadow_ice.vspeed = -4; 
} 

if(keyboard_check(vk_down) &amp;&amp; keyboard_check(vk_left)) 
{ 
    image_angle = 180 - 45; 
    hspeed = -4; 
    vspeed = 4; 
    
    obj_shadow_ice.image_angle = 180 - 45; 
    obj_shadow_ice.hspeed = -4; 
    obj_shadow_ice.vspeed = 4; 
} 

if(keyboard_check(vk_down) &amp;&amp; keyboard_check(vk_right)) 
{ 
    image_angle = 180 + 45; 
    hspeed = 4; 
    vspeed = 4; 
    
    obj_shadow_ice.image_angle = 180 + 45; 
    obj_shadow_ice.hspeed = 4; 
    obj_shadow_ice.vspeed = 4; 
}

// ANIMACIJA NA SPRAJT

if(keyboard_check_pressed(vk_up) || keyboard_check_pressed(vk_down) || keyboard_check_pressed(vk_left) || keyboard_check_pressed(vk_right))
{
    image_speed = 20;
}
if(keyboard_check_released(vk_up) || keyboard_check_released(vk_down) || keyboard_check_released(vk_left) || keyboard_check_released(vk_right) )
{
    image_speed = 0;
}

// SKOK

if(bJumpUp != 0)
    image_speed = 0;

if(bJumpUp == 1)
{
    image_xscale += 0.002;
    image_yscale += 0.002;
    
    obj_shadow_ice.x += 0.8;
    obj_shadow_ice.y += 0.8;
    
    if(image_xscale == scaleStart + jHeight)
        bJumpUp = 2;
}

if(bJumpUp == 2)
{
    image_xscale -= 0.002;
    image_yscale -= 0.002;
    
    obj_shadow_ice.x -= 0.8;
    obj_shadow_ice.y -= 0.8;
    
    if(image_xscale == scaleStart)
        bJumpUp = 0;
}


// Check collision

// Check for no collision (fall down)
if (place_free(x, y) &amp;&amp; image_xscale &lt;= 0.05)
{
    // Player dead
    alarm[1] = 1;
}

global.itemUnder = collision_point( x, y, fire_platform, false, true );

if (global.itemUnder &amp;&amp; image_xscale == 1)
{
    global.hvar--;
    health = floor(global.hvar/6);
    
    if (health &lt;= 0)
    {
    //    room_restart();
    /*    global.livesLeft --;
        
        if (global.livesLeft == 0)
        {
            room_goto(5);
        }
        else
        {*/
           script_execute(checkpointReset);
     //   }


        
    //    health = 100;
    }
}
</string>
